<%
   require "nokogiri"
%>

<h3>Map</h3>

<table>
  <tr>
    <td>Agency: </td>
    <td>
      <select name="agency" size="1">
        <% @agencies.each do |agen| %>
        <!-- agen[1] = agency_id; agen[2] = agency_name -->
        <option value="<%= agen[1] %>"> <%= agen[2] %> </option>
        <% end %>
      </select>
    </td>
  </tr>
  <tr>
    <td>Route: </td>
    <td>
      <select name="route" size="1">
      </select>
    </td>
  </tr>
</table>
